{
  "name": "antd-react-template",
  "version": "1.0.0",
  "description": "使用Antd提供的组件写一个Admin管理平台模板",
  "main": "server.js",
  "scripts": {
    "start": "babel-node server.js",
    "watch": "nodemon --exec babel-node -- server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boyongjiong/antd-react-template.git"
  },
  "keywords": [
    "Antd",
    "React",
    "Template"
  ],
  "dependencies": {
    "alt": "^0.18.6",
    "antd": "^2.5.1",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "swig": "^1.4.2",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-plugin-import": "^1.1.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "bower": "^1.8.0",
    "browser-sync": "^2.18.2",
    "browserify": "^13.1.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-if": "^2.0.2",
    "gulp-less": "^3.3.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "author": "boyongjiong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boyongjiong/antd-react-template/issues"
  },
  "homepage": "https://github.com/boyongjiong/antd-react-template#readme"
}
